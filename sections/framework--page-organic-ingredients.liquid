<section class="organic-ingredients">
         <div class="small-container">
            <h2>{{ section.settings.center_organic_ingredient_title }}</h2>
            <div class="row organic-ingredients-row">   
               <div class="col-3">
                  <div class="organic-img">
                     <img src="{{ section.settings.left_image | img_url: 'master' }}" alt="organic 1">
                  </div>
                  <h3>{{ section.settings.center_organic_ingredient_title1 }}</h3>
                  <p>{{ section.settings.center_organic_ingredient_desc1 }}</p>
               </div>
               <div class="col-3">
                  <div class="organic-img">
                     <img src="{{ section.settings.center_image | img_url: 'master' }}" alt="organic 2">
                  </div>
                  <h3>{{ section.settings.center_organic_ingredient_title2 }}</h3>
                  <p>{{ section.settings.center_organic_ingredient_desc2 }}</p>
               </div>
               <div class="col-3">
                  <div class="organic-img">
                     <img src="{{ section.settings.right_image | img_url: 'master' }}" alt="organic 3">
                  </div>
                  <h3>{{ section.settings.center_organic_ingredient_title3 }}</h3>
                  <p>{{ section.settings.center_organic_ingredient_desc3 }}</p>
               </div>
            </div>
         </div>
         <div class="container-fluid full-transp-container">
            <div class="row">  
               <div class="col-4 real-ingredient-text">
                  <h3>{{ section.settings.center_real_ingredient_title }}</h3>
                  <p>{{ section.settings.center_real_ingredient_description }}</p>
                  <a href="#">{{ section.settings.center_real_ingredient_button }}</a>
               </div>
               <div class="col-8 real-ingredient-img">
                  <div class="row">
                  <div class="col-7">
                    <div class="ingredient-box-one">
                      <ul>
                        <li><strong>{{ section.settings.left_ingredient_list_title }}</strong></li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_01 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_01 }}</h6>
                        </li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_02 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_02 }}</h6>
                        </li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_03 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_03 }}</h6>
                        </li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_04 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_04 }}</h6>
                        </li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_05 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_05 }}</h6>
                        </li>
                        <li>
                          <h5>{{ section.settings.left_ingredient_list_06 }}</h5>
                          <h6>{{ section.settings.left_ingredient_list_value_06 }}</h6>
                        </li>
                      </ul>
                    </div>
                  </div>
                  <div class="col-5">
                    <div class="ingredient-box">
                      <ul>
                        <li><strong>{{ section.settings.right_ingredient_list_title }}</strong></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_01 }}</a></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_02 }}</a></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_03 }}</a></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_04 }}</a></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_05 }}</a></li>
                        <li><a href="#">{{ section.settings.right_ingredient_list_06 }}</a></li>
                      </ul>
                    </div>
                  </div>
                  </div>
               </div>
            </div>
         </div>

</section>
